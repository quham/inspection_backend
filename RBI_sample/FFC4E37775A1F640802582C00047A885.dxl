<item name='DetCredible_Tx_6_1'><text>No</text></item>
<item name='Location_Tx_6_1'><text>-</text></item>
<item name='DetComment_Tx_6_1'><text>Not applicable.</text></item>
<item name='LocCount_Nb_6'><number>1</number></item>
<item name='Deterioration_Tx_7'><text>Mechanical damage</text></item>
<item name='DetCredible_Tx_7_1'><text>Yes</text></item>
<item name='Location_Tx_7_1'><text>Small bore branches.</text></item>
<item name='DetComment_Tx_7_1'><text>Potential for damage during maintenance activities.</text></item>
<item name='LocCount_Nb_7'><number>1</number></item>
